<h1 class="mt-4">Post</h1>

<div class="card mt-4">
  <div class="card-body">
    <h5 class="card-title"><%= @post.title %></h5>
    <p class="card-text"><%= @post.content %></p>
    <div class="btn btn-primary mt-4">
      <%= link_to "Posts", posts_path, class: "link-light" %>      
    </div>
  </div>
</div>
